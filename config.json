{
    "log-level": "info",
    "manager": "mixed",
    "standalone": false,
    "swanCluster": ["0.0.0.0:9999"],
    "scheduler": {
	    "mesos-masters": "127.0.0.1:5050",
	    "mesos-framwork-user": "root",
	    "hostname": "",
	    "local-healthcheck": false
    },
    "dns": {
	    "enable-dns-proxy": false,
	    "domain": "swan",
	    "recurse_on": true,
	    "ip": "0.0.0.0",
	    "port": 53,
	    "soarname": "",
	    "soamname": "",
	    "soaserial": 0,
	    "soarefresh": 0,
	    "soaretry": 0,
	    "soaexpire": 0,
	    "ttl": 0,
	    "resolvers": ["114.114.114.114"],
	    "exchange_timeout": 1
    },
    "httpListener": {
	    "addr": "0.0.0.0:9999" ,
	    "sock": "./data/swan.sock"
    },
    "ipam": {
	    "store_path": "./data/"
    },
    "raft": {
	    "cluster": "http://127.0.0.1:2111",
	    "raftid": 1,
	    "store_path": "./data/"
    },
    "janitor": {
	    "enableProxy": true,
	    "listenerMode": "single_port",
	    "port": "9998"
    },
    "apiServerAddr": ":5015"
}
