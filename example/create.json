{
  "name": "nginx-demo",
  "cmd": null,
  "args": null,
  "cpus": 0.01,
  "mem": 32,
  "disk": 0,
  "runAs": "xcm",
  "priority": 0,
  "instances": 3,
  "constraints": [
    {
      "attribute": "vcluster",
      "operator": "==",
      "value": "bbklab"
    }
  ],
  "container": {
    "docker": {
      "image": "nginx",
      "network": "bridge",
      "forcePullImage": false,
      "privileged": true,
      "parameters": [
        {
          "key": "ipc",
          "value": "host"
        },
        {
          "key": "workdir",
          "value": "/data"
        }
      ],
      "portMappings": [
        {
          "name": "web",
          "protocol": "tcp",
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    },
    "type": "DOCKER",
    "volumes": [
      {
        "hostPath": "/home",
        "containerPath": "/data",
        "mode": "RW"
      }
    ]
  },
  "env": {
    "WORDPRESS_DB_HOST": "dbhost",
    "WORDPRESS_DB_PASSWORD": "password"
  },
  "uris": [
    
  ],
  "label": {
    "USER_ID": "1",
    "APP": "wordpress"
  },
  "healthCheck": {
    "protocol": "http",
    "path": "/",
    "delaySeconds": 2,
    "gracePeriodSeconds": 5,
    "intervalSeconds": 1,
    "portName": "web",
    "timeoutSeconds": 1,
    "consecutiveFailures": 5
  },
  "proxy": {
    "enabled": true,
    "alias": "g.cn",
	"listen": 99,
	"sticky": false
  },
  "deploy": {
    "step": 10,
    "onfailure": "stop"
  }
}
