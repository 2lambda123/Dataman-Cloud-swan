{
  "appID": "nginx0055",
  "cmd": null,
  "cpus": 0.01,
  "mem": 5,
  "disk": 0,
  "instances": 2,
  "container": {
    "docker": {
      "image": "nginx",
      "network": "swan",
      "forcePullImage": false,
      "privileged": true,
      "parameters": [
        {
            "key": "label",
	    "value": "APP_ID=nginx"
        }
      ],
      "portMappings": []
    },
    "type": "DOCKER",
    "volumes": [
      {
        "hostPath": "/home",
        "containerPath": "/data",
        "mode": "RW"
      }
    ]
  },
  "env": {
    "DB": "mysql"
  },
  "labels": {
    "USER_ID": "1"
  },
  "killPolicy": {
    "duration": 5
  },
  "healthChecks": [ ],

  "mode": "fixed",
  "ip": ["192.168.1.210","192.168.1.211"]
}
