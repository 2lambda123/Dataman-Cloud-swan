{
  "name": "nginx009",
  "cmd": null,
  "args": null,
  "cpus": 0.01,
  "mem": 6,
  "disk": 0,
  "runAs": "xcm",
  "priority": 100,
  "instances": 1,
  "constraints": [],
  "container": {
    "docker": {
      "image": "nginx",
      "network": "bridge",
      "forcePullImage": false,
      "privileged": true,
      "parameters": [
        {
            "key": "label",
	    "value": "APP_ID=wordpress"
        }
      ],
      "portMappings": [
	      {
		      "name": "web",
		      "protocol": "tcp",
		      "containerPort": 80,
		      "hostPort": 80
	      }
      ]
    },
    "type": "DOCKER",
    "volumes": [
      {
        "hostPath": "/home",
        "containerPath": "/data",
        "mode": "RW"
      }
    ]
  },
  "env": {
    "WORDPRESS_DB_HOST": "192.168.1.210",
    "WORDPRESS_DB_PASSWORD": "root"
  },
  "uris": [
  ],
  "label": {
    "USER_ID": "1"
  },
  "proxy": {
	    "enabled": false,
	    "alias": ""
  }
}
